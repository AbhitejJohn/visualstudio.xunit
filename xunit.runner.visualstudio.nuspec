<?xml version="1.0" encoding="utf-8"?>
<package>
  <metadata>
    <id>xunit.runner.visualstudio</id>
    <version>0.99.8</version>
    <title>xUnit.net [Visual Studio]</title>
    <authors>James Newkirk, Brad Wilson</authors>
    <description>Includes support for running xUnit.net v1 and v2 tests from Visual Studio 2012+.</description>
    <language>en-US</language>
    <projectUrl>https://github.com/xunit/xunit</projectUrl>
    <iconUrl>https://raw.githubusercontent.com/xunit/media/master/logo-512-transparent.png</iconUrl>
    <licenseUrl>https://raw.githubusercontent.com/xunit/xunit/master/license.txt</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
  </metadata>
  <files>
    <!-- Discovery (all platforms) -->
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.runner.utility.dll"                  target="build\_common\xunit.runner.utility.dll" />
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.runner.visualstudio.testadapter.dll" target="build\_common\xunit.runner.visualstudio.testadapter.dll" />
    <file src="xunit.runner.visualstudio.testadapter\bin\Release\xunit.runner.visualstudio.testadapter.pdb" target="build\_common\xunit.runner.visualstudio.testadapter.pdb" />

    <!-- Execution (desktop) -->
    <file src="build\_._" target="lib\net20\_._" />

    <!-- Execution (Win8) -->
    <file src="build\_._"                                                                                        target="lib\win8\_._" />
    <file src="build\xunit.runner.visualstudio.wpa81+win8.props"                                                 target="build\win8\xunit.runner.visualstudio.props" />
    <file src="build\xunit.runner.visualstudio.wpa81+win8.targets"                                               target="build\win8\xunit.runner.visualstudio.targets" />
    <file src="xunit.runner.visualstudio.testadapter.win8\bin\Release\xunit.runner.utility.dll"                  target="build\win8\device\xunit.runner.utility.dll" />
    <file src="xunit.runner.visualstudio.testadapter.win8\bin\Release\xunit.runner.visualstudio.testadapter.dll" target="build\win8\device\xunit.runner.visualstudio.testadapter.dll" />
    <file src="xunit.runner.visualstudio.testadapter.win8\bin\Release\xunit.runner.visualstudio.testadapter.pdb" target="build\win8\device\xunit.runner.visualstudio.testadapter.pdb" />

    <!-- Execution (WPA81) -->
    <file src="build\_._"                                                                                         target="lib\wpa81\_._" />
    <file src="build\xunit.runner.visualstudio.wpa81+win8.props"                                                  target="build\wpa81\xunit.runner.visualstudio.props" />
    <file src="build\xunit.runner.visualstudio.wpa81+win8.targets"                                                target="build\wpa81\xunit.runner.visualstudio.targets" />
    <file src="xunit.runner.visualstudio.testadapter.wpa81\bin\Release\xunit.runner.utility.dll"                  target="build\wpa81\device\xunit.runner.utility.dll" />
    <file src="xunit.runner.visualstudio.testadapter.wpa81\bin\Release\xunit.runner.utility.pri"                  target="build\wpa81\device\xunit.runner.utility.pri" />
    <file src="xunit.runner.visualstudio.testadapter.wpa81\bin\Release\xunit.runner.visualstudio.testadapter.dll" target="build\wpa81\device\xunit.runner.visualstudio.testadapter.dll" />
    <file src="xunit.runner.visualstudio.testadapter.wpa81\bin\Release\xunit.runner.visualstudio.testadapter.pri" target="build\wpa81\device\xunit.runner.visualstudio.testadapter.pri" />
    <file src="xunit.runner.visualstudio.testadapter.wpa81\bin\Release\xunit.runner.visualstudio.testadapter.pdb" target="build\wpa81\device\xunit.runner.visualstudio.testadapter.pdb" />

    <!-- Source -->
    <file src="xunit.runner.visualstudio.testadapter*\**\*.cs" target="src" />
  </files>
</package>